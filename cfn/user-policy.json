{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "codepipeline:*",
        "codebuild:**",
        "codecommit:*",
        "s3:CreateBucket",
        "s3:PutObject",
        "s3:ListBucket",
        "s3:*",
        "events:*",
        "logs:Describe*",
        "logs:Get*",
        "iam:AttachRolePolicy",
        "iam:CreateRole",
        "iam:PutRolePolicy",
        "iam:CreatePolicy",
        "iam:CreatePolicyVersion",
        "iam:DeletePolicyVersion",
        "iam:DeletePolicy",
        "iam:DeleteRole",
        "iam:DeleteRolePolicy",
        "iam:PassRole",
        "cloudformation:*"
      ],
      "Resource": "*",
      "Effect": "Allow"
    }
  ]
}